<Window x:Class="MBTI.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MBTI"
        mc:Ignorable="d"
        Title="StartWindow" Height="450" Width="800" MinHeight="450" MinWidth="800">
    <Grid>
        <Button x:Name="BTStart" Content="Начать" HorizontalAlignment="Center" Margin="0,0,0,30" VerticalAlignment="Bottom" Width="75" FontSize="16"  Padding="5" Click="BTStart_Click" />
        <Border BorderBrush="LightSkyBlue"  BorderThickness="2" Padding="5" Margin="2,30,2,80">
            <ScrollViewer>
                <TextBlock x:Name="TBStart" TextWrapping="Wrap" Text="" FontSize="14"/>
            </ScrollViewer>
        </Border>
    </Grid>
</Window>

